{
	"name": "alexswensen.io-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prettier:check": "prettier --plugin-search-dir . --check .",
		"test:e2e": "playwright test",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"playwright:ci": "playwright test",
		"playwright:ui": "playwright test --ui"
	},
	"devDependencies": {
		"@playwright/test": "^1.38.1",
		"@sentry/vite-plugin": "^0.7.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.26.0",
		"@testing-library/jest-dom": "^6.1.3",
		"@testing-library/svelte": "^4.0.4",
		"@types/node": "^20.7.2",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"daisyui": "^2.51.6",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.33.2",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-plugin-compile-time": "^0.2.1",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@sentry/node": "^7.72.0",
		"@sentry/svelte": "^7.72.0",
		"@sentry/tracing": "^7.72.0"
	}
}
