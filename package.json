{
	"name": "alexswensen.io-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prettier:check": "prettier --plugin-search-dir . --check .",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma:seed": "ts-node scripts/seed.ts"
	},
	"prisma": {
		"seed": "ts-node --esm prisma/seed.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@sentry/vite-plugin": "^0.7.2",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/kit": "^1.15.10",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.27.2",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tailwindcss": "^3.3.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.4",
		"prisma": "^4.13.0",
		"vitest": "^0.30.1"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.13.0",
		"@sentry/node": "^7.50.0",
		"@sentry/svelte": "^7.50.0",
		"@sentry/tracing": "^7.50.0"

	}
}
