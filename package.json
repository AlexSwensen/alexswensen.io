{
	"name": "alexswensen.io-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prettier:check": "prettier --plugin-search-dir . --check .",
		"test:e2e": "playwright test",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"playwright:ci": "playwright test",
		"playwright:ui": "playwright test --ui"
	},
	"devDependencies": {
		"@playwright/test": "^1.39.0",
		"@sentry/vite-plugin": "^0.7.2",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.6",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/svelte": "^4.0.4",
		"@types/node": "^20.8.7",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite": "^2.2.0",
		"flowbite-svelte": "^0.44.19",
		"gray-matter": "^4.0.3",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.1",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.12",
		"vite-plugin-compile-time": "^0.2.1",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@sentry/node": "^7.74.1",
		"@sentry/svelte": "^7.74.1",
		"@sentry/tracing": "^7.74.1"
	}
}
